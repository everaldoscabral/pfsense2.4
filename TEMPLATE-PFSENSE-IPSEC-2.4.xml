<?xml version="1.0" encoding="UTF-8"?>
<zabbix_export>
    <version>5.0</version>
    <date>2020-06-25T14:56:09Z</date>
    <groups>
        <group>
            <name>Templates/Applications</name>
        </group>
    </groups>
    <templates>
        <template>
            <template>TEMPLATE-PFSENSE-IPSEC-2.4</template>
            <name>TEMPLATE-PFSENSE-IPSEC-2.4</name>
            <groups>
                <group>
                    <name>Templates/Applications</name>
                </group>
            </groups>
            <applications>
                <application>
                    <name>IPSec</name>
                </application>
            </applications>
            <discovery_rules>
                <discovery_rule>
                    <name>IPSec Tunnel Discovery</name>
                    <key>ipsec.discover</key>
                    <delay>1d</delay>
                    <filter>
                        <conditions>
                            <condition>
                                <macro>{#DESCRIPTION}</macro>
                                <value>@Excluir IPSec Pfsense</value>
                                <formulaid>A</formulaid>
                            </condition>
                        </conditions>
                    </filter>
                    <lifetime>0d</lifetime>
                    <item_prototypes>
                        <item_prototype>
                            <name>Bytes IN: {#DESCRIPTION} -  {#TUNNEL}</name>
                            <key>ipsec.traffic[{#TUNNEL},&quot;bytesIn&quot;]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>90d</trends>
                            <units>bytes</units>
                            <applications>
                                <application>
                                    <name>IPSec</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Bytes OUT: {#DESCRIPTION} -  {#TUNNEL}</name>
                            <key>ipsec.traffic[{#TUNNEL},&quot;bytesOut&quot;]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>90d</trends>
                            <units>bytes</units>
                            <applications>
                                <application>
                                    <name>IPSec</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>PKTS IN: {#DESCRIPTION} -  {#TUNNEL}</name>
                            <key>ipsec.traffic[{#TUNNEL},&quot;pktsIn&quot;]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>90d</trends>
                            <units>pkts</units>
                            <applications>
                                <application>
                                    <name>IPSec</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>PKTS Out: {#DESCRIPTION} -  {#TUNNEL}</name>
                            <key>ipsec.traffic[{#TUNNEL},&quot;pktsOut&quot;]</key>
                            <delay>2m</delay>
                            <history>30d</history>
                            <trends>90d</trends>
                            <units>pkts</units>
                            <applications>
                                <application>
                                    <name>IPSec</name>
                                </application>
                            </applications>
                        </item_prototype>
                        <item_prototype>
                            <name>Tunnel: {#DESCRIPTION} - {#TUNNEL}</name>
                            <key>ipsec.tunnel[{#TUNNEL}]</key>
                            <delay>30s</delay>
                            <history>30d</history>
                            <trends>90d</trends>
                            <applications>
                                <application>
                                    <name>IPSec</name>
                                </application>
                            </applications>
                            <valuemap>
                                <name>pfSense IPSec Status</name>
                            </valuemap>
                            <trigger_prototypes>
                                <trigger_prototype>
                                    <expression>{last(0)}&lt;&gt;0</expression>
                                    <name>Tunnel: {#DESCRIPTION} - {#TUNNEL}  down</name>
                                    <priority>HIGH</priority>
                                    <tags>
                                        <tag>
                                            <tag>tunnel_name</tag>
                                            <value>{#TUNNEL}</value>
                                        </tag>
                                        <tag>
                                            <tag>tunnel_state</tag>
                                            <value>down</value>
                                        </tag>
                                    </tags>
                                </trigger_prototype>
                            </trigger_prototypes>
                        </item_prototype>
                    </item_prototypes>
                    <graph_prototypes>
                        <graph_prototype>
                            <name>{#DESCRIPTION} - {#TUNNEL} Packages</name>
                            <graph_items>
                                <graph_item>
                                    <color>2774A4</color>
                                    <item>
                                        <host>TEMPLATE-PFSENSE-IPSEC-2.4</host>
                                        <key>ipsec.traffic[{#TUNNEL},&quot;pktsIn&quot;]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>A54F10</color>
                                    <item>
                                        <host>TEMPLATE-PFSENSE-IPSEC-2.4</host>
                                        <key>ipsec.traffic[{#TUNNEL},&quot;pktsOut&quot;]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                        <graph_prototype>
                            <name>{#DESCRIPTION} - {#TUNNEL} Traffic</name>
                            <graph_items>
                                <graph_item>
                                    <color>F63100</color>
                                    <item>
                                        <host>TEMPLATE-PFSENSE-IPSEC-2.4</host>
                                        <key>ipsec.traffic[{#TUNNEL},&quot;bytesIn&quot;]</key>
                                    </item>
                                </graph_item>
                                <graph_item>
                                    <sortorder>1</sortorder>
                                    <color>2774A4</color>
                                    <item>
                                        <host>TEMPLATE-PFSENSE-IPSEC-2.4</host>
                                        <key>ipsec.traffic[{#TUNNEL},&quot;bytesOut&quot;]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                        <graph_prototype>
                            <name>{#DESCRIPTION} status</name>
                            <graph_items>
                                <graph_item>
                                    <color>1A7C11</color>
                                    <item>
                                        <host>TEMPLATE-PFSENSE-IPSEC-2.4</host>
                                        <key>ipsec.tunnel[{#TUNNEL}]</key>
                                    </item>
                                </graph_item>
                            </graph_items>
                        </graph_prototype>
                    </graph_prototypes>
                </discovery_rule>
            </discovery_rules>
        </template>
    </templates>
    <value_maps>
        <value_map>
            <name>pfSense IPSec Status</name>
            <mappings>
                <mapping>
                    <value>0</value>
                    <newvalue>Established</newvalue>
                </mapping>
                <mapping>
                    <value>1</value>
                    <newvalue>Disconnected</newvalue>
                </mapping>
            </mappings>
        </value_map>
    </value_maps>
</zabbix_export>
